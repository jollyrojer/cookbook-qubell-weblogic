print 'DEPLOY <%= node[:weblogic][:lib][:name] %> LIBRARY';

connect('<%= node[:weblogic][:user] %>','<%= node[:weblogic][:password] %>','t3://<%= node[:weblogic][:domain_name] %>:<%= node[:weblogic][:port] %>')
deploy('<%= @ctx[:lib_name] %>','<%= @ctx[:lib_source] %>',targets='<%= @ctx[:wl_target] %>',libraryModule='<%= @ctx[:is_module] %>')

exit()
