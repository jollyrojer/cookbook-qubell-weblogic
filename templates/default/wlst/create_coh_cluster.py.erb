print 'CREATING <%= node[:weblogic][:coh_cluster][:name] %> COHERENCE CLUSTER';

edit()
startEdit()
cmo.createCoherenceClusterSystemResource('<%= node[:weblogic][:coh_cluster][:name] %>')
cd('/CoherenceClusterSystemResources/<%= node[:weblogic][:coh_cluster][:name] %>')
set('Targets',jarray.array([ObjectName('com.bea:Name=<%= node[:weblogic][:server][:name] %>.join(','),Type=Server')],ObjectName))
cd('Resource/<%= node[:weblogic][:coh_cluster][:name] %>/CoherenceClusterParams/<%= node[:weblogic][:coh_cluster][:name] %>')
cmo.setUnicastPortAutoAdjust(<%= node[:weblogic][:coh_cluster][:unicast_port_autoadjust] %>)
cmo.setUnicastListenAddress('<%= node[:weblogic][:coh_cluster][:unicast_address] %>')
cmo.setUnicastListenPort(<%= nodenode[:weblogic][:coh_cluster][:unicast_port] %>)
activate()
disconnect()
exit()
